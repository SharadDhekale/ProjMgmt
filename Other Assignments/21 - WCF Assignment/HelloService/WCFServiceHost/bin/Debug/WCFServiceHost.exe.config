<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="mexBehavior" name="HelloService.HelloService">
        <endpoint address="HelloService" binding="basicHttpBinding" name="usingBasicHttp"
          contract="HelloService.IHelloService" />
        <endpoint address="HelloService" binding="netTcpBinding" name="usingNetTcp"
          contract="HelloService.IHelloService" />
        <endpoint address="mex" binding="mexHttpBinding" name="usingMex"
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:2385/" />
            <add baseAddress="net.tcp://localhost:8090/" />
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="mexBehavior" name="CalculatorService.Calculator">
        <endpoint address="Calculator" binding="basicHttpBinding" name="usingBasicHttp"
          contract="CalculatorService.ICalculator" />
        <endpoint address="mex" binding="mexHttpBinding" name="usingMex"
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:2300/" />
           </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>