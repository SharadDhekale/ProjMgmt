<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="mexBehavior" name="WeatherService.WeatherInfo">
        <endpoint address="WeatherService" binding="basicHttpBinding" name="usingBasicHttp"
          contract="WeatherService.IWeatherInfo" />
        <endpoint address="mex" binding="mexHttpBinding" name="usingMex"
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:2525/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>